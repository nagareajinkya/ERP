{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST:localhost};Port=${DB_PORT:5432};Database=${DB_NAME:trading_db};Username=${DB_USERNAME:postgres};Password=${DB_PASSWORD:manager}"
  },
